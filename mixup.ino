bool checkAlarm() {
  struct tm timeinfo;
  if(!getLocalTime(&timeinfo)){
    Serial.println("Failed to obtain time");
  }
  int hourCheck = timeinfo.tm_hour;
  int minuteCheck = timeinfo.tm_min;
  for (int i = 0; i < indexAlarmClocks; i++) {
      String hourTemp;
      hourTemp = alarmClocks[i*6];
      hourTemp += alarmClocks[1 + i*6];
      String minuteTemp;
      minuteTemp = alarmClocks[3 + i*6];
      minuteTemp += alarmClocks[4 + i*6];
      int hourToInt = hourTemp.toInt();
      int minuteToInt = minuteTemp.toInt();
      if (hourToInt == hourCheck && minuteToInt == minuteCheck) return 1;
  }
  return 0;
}

